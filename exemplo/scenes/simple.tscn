func get_drag_data(position):
	
	var preview = self.duplicate()
	# transparÃªncia
	preview.modulate.a = .5
	# centralizar ao mouse
	preview.get_node(\"color\").rect_position = -self.rect_size / 2
	# esconde o fundo
	preview.get_node(\"bkg\").hide()

	set_drag_preview(preview)
	
	return self
